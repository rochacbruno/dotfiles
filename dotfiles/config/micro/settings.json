{
    "autoclose": false,
    "backup": false,
    "colorcolumn": 100,
    "colorscheme": "catppuccin-mocha",
    "diffgutter": true,
    "ft:makefile": {
        "tabstospaces": false
    },
    "ft:yaml": {
        "rmtrailingws": false
    },
    "hlsearch": true,
    "ignorecase": false,
    "indentchar": "",
    "lsp.ignoreTriggerCharacters": "completion,signature",
    "lsp.server": "python=pyls,go=gopls,typescript=deno lsp,rust=rust-analyzer",
    "mkparents": true,
    "pluginchannels": [
        "https://raw.githubusercontent.com/micro-editor/plugin-channel/master/channel.json"
    ],
    "pluginrepos": [],
    "rmtrailingws": true,
    "rust": true,
    "rust-plugin.linter-cargo-check": false,
    "rust-plugin.linter-clippy": false,
    "rust-plugin.onsave-build": false,
    "rust-plugin.onsave-fmt": false,
    "rust-plugin.rustfmt-backup": false,
    "rust-plugin.tool-cargo-rustc": false,
    "savecursor": true,
    "saveundo": true,
    "scrollbar": true,
    "softwrap": true,
    "tabmovement": true,
    "tabstospaces": true,
    "wordwrap": true
}
